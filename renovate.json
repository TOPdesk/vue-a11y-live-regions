{
	"$schema": "https://docs.renovatebot.com/renovate-schema.json",
	"extends": [
		"config:recommended",
		"helpers:pinGitHubActionDigests"
	],
	"enabledManagers": [
		"npm",
		"github-actions"
	],
	"labels": [
		"dependencies"
	],
	"rebaseWhen": "behind-base-branch",
	"lockFileMaintenance": {
		"enabled": true
	},
	"timezone": "UTC"
}
